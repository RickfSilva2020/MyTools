from kivy.app import App
from kivy.uix.boxlayout import BoxLayout

class Cmotores(BoxLayout):
    def Calc_motores(self,**kwargs):
        pass
    def escolhaPotencia(self):
        potencia = self.ids.pot.text
        print(potencia)
        
    
    
class Telacalc(App):
    def build(self):
        return Cmotores()
        
Telacalc().run()